{
    "name": "rkooistra/super-eloquent-repository",
    "description": "A concrete repository for Eloquent models to allow quick implementation of common functionalities, such as CRUD and search methods. Save yourself the boring stuff.",
    "type": "library",
    "version":"1.0.2-stable",
    "require": {
        "laravel/framework": ">=5.5",
        "php": ">=7.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5"
    },
    "repositories": [
        {
            "type": "github",
            "url": "https://github.com/KakaCarrotCake/super-eloquent-repository"
        }
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Remy Kooistra",
            "email": "remykooistra77@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "RKooistra\\SuperEloquentRepository\\": "src",
            "RKooistra\\SuperEloquentRepository\\Tests\\": "tests"
        },
        "files": []
    }
}
